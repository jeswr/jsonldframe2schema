{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "@type": {
      "const": "Event"
    },
    "name": {
      "type": "string"
    },
    "startDate": {
      "type": "string",
      "format": "date-time"
    },
    "attendeeCount": {
      "type": "integer"
    }
  },
  "required": ["@type", "name", "startDate", "attendeeCount"],
  "additionalProperties": true
}
