{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "@type": {
      "const": "Event"
    },
    "name": {
      "type": "string"
    },
    "startDate": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "xsd:dateTime"
        }
      },
      "required": ["@type"],
      "additionalProperties": true
    },
    "attendeeCount": {
      "type": "object",
      "properties": {
        "@type": {
          "const": "xsd:integer"
        }
      },
      "required": ["@type"],
      "additionalProperties": true
    }
  },
  "required": ["@type", "name", "startDate", "attendeeCount"],
  "additionalProperties": true
}
